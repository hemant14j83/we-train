<div class="col-xs-12 col-md-12 col-sm-12">
<% if expertise.expertise_in!=nil %>
	<div class="qbox">
			<table class="table">
				<tr>
					<th><b>Expertise In</b></th>
					<th><b>Prof Expertise Info</b></th>
					<th><b>Ref Name</b></th>
					<th><b>Ref Number</b></th>
					<th><b>Ref Email</b></th>
					<th><b>Verified or Not?</b></th>
				</tr>
				<tr>
					<td><%= expertise.expertise_in %></td>
					<td><%= expertise.professional_expertise_info %></td>
					<td><%= expertise.reference_name %><br><%= expertise.reference_name1 %></td>
					<td><%= expertise.reference_number %><br><%= expertise.reference_number1 %></td>
					<td><%= expertise.reference_email %><br><%= expertise.reference_email1 %></td>
					<td><%= expertise.expertise_verified_status %></td>
				</tr>
			</table>
		    <div align="right">
		    	<%= link_to 'Remove', expertise, method: :delete, data: { confirm: 'Are you sure?' }, :class=>"btn btn-danger btn-sm" %>
		    </div>
	</div>
	<%end%>
</div>