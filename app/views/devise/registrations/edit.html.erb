<div class="col-xs-12">
  <div class="col-xs-3"></div>
  <div class="col-xs-9">
      <h3>Update Profile <%= resource_name.to_s.humanize %></h3>
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f| %>
        <%= devise_error_messages! %>
        <div class="form-horizontal">
          <div class="form-group col-xs-8">
            <%= f.label :email %>
            <%= f.email_field :email, autofocus: true, :class=>"form-control" %>
          </div>
          <div class="form-group col-xs-8">
            <%= f.label :full_name %>
            <%= f.text_field :full_name, :class=>"form-control" %>
          </div>
          <!--<div class="form-group col-xs-8">
            <%= f.label :current_password %>
            <%= f.password_field :current_password, autocomplete: false, :class=>"form-control" %>
          </div>
          <div class="form-group col-xs-8">
            <%= f.password_field :password, autocomplete: "off", :class=>"form-control" %>
          </div>
          <div class="form-group col-xs-8">
            <%= f.password_field :password_confirmation, autocomplete: "off", :class=>"form-control" %>
          </div>-->
          <div class="form-group col-xs-8">
            <%= f.label :primary_number %>
            <%= f.text_field :primary_number, :class=>"form-control" %>
          </div>
          <div class="form-group col-xs-8">
            <%= f.label :birth_date %>
            <%= f.date_select :birth_date, :start_year=>1930, :end_year=>2015, :class=>"form-control" %>
          </div>
          <div class="form-group col-xs-8">
            <%= f.label :gender %>
            <%= f.select :gender, ['Select Gender','Male','Female'], :class=>"form-control" %>
          </div>
          <div class="form-group col-xs-8">
            <%= f.label :address %>
            <%= f.text_area(:address, size: '50x4', :class=>"form-control")%>
          </div>
          <div class="form-group col-xs-8">
            <%= f.label :city %>
            <%= f.text_field :city, :class=>"form-control" %>
          </div>
          <div class="form-group col-xs-8">
            <%= f.label :state %>
            <%= f.text_field :state, :class=>"form-control" %>
          </div>          
          <div class="form-group col-xs-8">
            <%= f.label :country %>
            <%= f.select :country, ['Select Country','India'], :class=>"form-control" %>
          </div>
          <div class="form-group col-xs-8">
            <%= f.label :profile_status %>
            <%= f.text_field :profile_status, :class=>"form-control" %>
          </div>
          <div class="form-group col-xs-8">
            <%= f.label :subscribed_plan %>
            <%= f.text_field :subscribed_plan, :class=>"form-control" %>
          </div>
          <div class="form-group col-xs-8">
            <%= f.label :busy_from %>
            <%= f.date_select :busy_from, :start_year=>2015, :end_year=>2015, :class=>"form-control" %>
          </div>
          <div class="form-group col-xs-8">
            <%= f.label :busy_till %>
            <%= f.date_select :busy_till, :start_year=>2015, :end_year=>2016,  :class=>"form-control" %>
          </div>
          <div class="form-group col-xs-8">
            <%= f.submit "Update Details", :class=>"form-control btn btn-success" %>
          </div>
        </div>
        <%end%>
  </div>
